{
  "product-summary": {
    "component": "ProductSummaryLegacy",
    "required": ["add-to-list-btn"]
  },
  "product-summary.unstable--flex": {
    "allowed": [
      "unstable--product-summary-column",
      "product-summary-attachment-list",
      "product-summary-buy-button",
      "product-summary-description",
      "product-summary-image",
      "product-summary-name",
      "product-summary-price",
      "product-summary-space"
    ],
    "component": "ProductSummaryCustom",
    "composition": "children"
  },
  "unstable--product-summary-column": {
    "allowed": [
      "product-summary-attachment-list",
      "product-summary-buy-button",
      "product-summary-description",
      "product-summary-image",
      "product-summary-name",
      "product-summary-price",
      "product-summary-space"
    ],
    "component": "Column",
    "composition": "children"
  },
  "product-summary-space": {
    "component": "Spacer"
  },
  "product-summary-attachment-list": {
    "component": "ProductSummaryAttachmentList"
  },
  "product-summary-buy-button": {
    "component": "ProductSummaryBuyButton"
  },
  "product-summary-description": {
    "component": "ProductSummaryDescription"
  },
  "product-summary-image": {
    "component": "ProductSummaryImage"
  },
  "product-summary-name": {
    "component": "ProductSummaryName"
  },
  "product-summary-price": {
    "component": "ProductSummaryPrice"
  }
}
